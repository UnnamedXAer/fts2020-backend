{
  "name": "fts-backend",
  "version": "1.0.0",
  "description": "FTS-2020 backend",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "echo \"Error: no start script\" && exit 1",
    "migrate:make": "knex -x ts --cwd src/db/ migrate:make",
    "migrate:latest": "knex --cwd src/db/ migrate:latest",
    "migrate:rollback": "knex --cwd src/db/ migrate:rollback --all",
    "migrate:down": "knex --cwd src/db/ migrate:down",
    "migrate:list": "knex --cwd src/db/ migrate:list",
    "seed:make": "knex -x ts --cwd src/db/ seed:make",
    "seed:run": "knex --cwd src/db/ seed:run"
  },
  "author": "Kamil",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "connect-session-knex": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "express-winston": "^4.0.2",
    "http-errors": "^1.7.3",
    "knex": "^0.20.8",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.1",
    "ts-node": "^8.6.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/http-errors": "^1.6.3",
    "@types/knex": "^0.16.1",
    "@types/passport": "^1.0.2",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-local": "^1.0.33",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  }
}
