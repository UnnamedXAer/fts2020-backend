{
  "name": "fts-backend",
  "version": "1.0.0",
  "description": "FTS-2020 backend",
  "main": "dist/server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "echo \"Error: no start script\" && exit 1",
    "migrate:make": "cross-env TRACE_LEVEL=\"Silly\" knex -x ts --cwd src/db/ migrate:make",
    "migrate:up": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:up",
    "migrate:latest": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:latest",
    "migrate:down-to": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:down",
    "migrate:down": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:down",
    "migrate:rollback": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:rollback",
    "migrate:rollback-all": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:rollback --all",
    "migrate:list": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ migrate:list",
    "seed:make": "cross-env TRACE_LEVEL=\"Silly\" knex -x ts --cwd src/db/ seed:make",
    "seed:run": "cross-env TRACE_LEVEL=\"Silly\" knex --cwd src/db/ seed:run"
  },
  "author": "Kamil",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "connect-session-knex": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "express-winston": "^4.0.2",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "knex": "^0.20.8",
    "moment": "^2.26.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "ts-node": "^8.10.2",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/http-errors": "^1.6.3",
    "@types/knex": "^0.16.1",
    "@types/moment": "^2.13.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.2",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^8.0.0",
    "@types/validator": "^13.0.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2",
    "typescript": "^3.9.5"
  }
}
